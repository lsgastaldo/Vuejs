rd\n                            elevation='1' \n                        >\n                            <v-toolbar\n                                color=\"green\"\n                                class=\"white--text\"\n                                height=\"60px\"\n                                flat\n                            >\n                            <v-toolbar-title> Login </v-toolbar-title>\n                            <v-spacer></v-spacer>\n                            <v-icon dark right>mdi-login</v-icon>\n                            </v-toolbar>\n                            <v-card-text>\n                                <v-form>\n                                    <v-text-field\n                                        v-model=\"credentials.username\"\n                                        label=\"Login\"\n                                        name=\"login\"\n                                        type=\"text\"\n                                        :rules=\"rules.username\"\n                                        single-line\n                                    ></v-text-field>\n                                    <v-text-field\n                                        v-model=\"credentials.password\"\n                                        label=\"Password\"\n                                        name=\"password\"\n                                        type=\"password\"\n                                        :rules=\"rules.password\"\n                                        single-line\n                                    ></v-text-field>\n                                </v-form>\n                            </v-card-text>\n                            <v-card-actions>\n                                <v-btn \n                                    class=\"white--text\"\n                                    color=\"yellow\"\n                                    type=\"text\"\n                                    block\n                                    depressed\n                                    @click=\"login()\"\n                                >Login\n                                </v-btn>\n                            </v-card-actions>\n                            <v-card-actions>\n                                <v-btn\n                                    class=\"white--text\"\n                                    color=\"red\"\n                                    block\n                                    depressed\n                                    @click=\"register()\"\n                                >Create account!\n                                </v-btn>\n                            </v-card-actions>\n                            <p class=\"text-center\"><a href=\"######\">Forgot your password?</a></p>\n                        </v-card>\n                    </v-flex>\n                </v-layout>\n            </v-container>\n        </v-content>\n        \n        <!-- FOOTER -->\n        <v-footer\n            app\n            color=\"green\"\n            class=\"white--text\"\n        > \n            <span>Instituto Federal Catarinense - Araquari</span>\n            <v-spacer></v-spacer>\n            <span>&copy; versao 1.0.53.2</span>\n        </v-footer>\n    </v-app>\n</template>\n\n<script>\nimport router from \"@/router/\";\nimport axios from \"axios\";\n\n \nexport default {  \n  name: \"Login\",\n  props: {\n    source: String,\n  }, \n  data: () => ({\n    credentials: {},\n    errorShow: false,\n    rules: {\n      username: [v => !!v || \"Usuário é obrigatório.\"],\n      password: [\n        v => !!v || \"Senha é obrigatória.\",\n        v => (v && v.length >= 3) || \"A senha deve ser maior que 5 caracteres.\"\n      ]\n    }\n  }),\n  methods: {\n    login() {\n      if (this.$refs.form.validate()) {\n        axios\n          .post(\"http://127.0.0.1:80/servidorPHP/index.php/login\", this.credentials)\n          .then(res => {\n            this.$session.start();\n            this.$session.set(\"token\", res.data.token);\n            router.push(\"/home\")\n          })\n          .catch(e => { \n            console.log(e)\n            this.errorShow = true\n          });\n      }\n    },\n    register(){\n        router.push(\"/login\")\n    }\n  }\n};\n</script>"]}]}